<%= simple_form_for @translation, html: { class: 'form-horizontal'} do |f| %>
  <%= devise_error_messages! %>
  
  <%= f.input :locale %>
  <%= f.input :translation_key %>
  <%= f.input :value %>
  <%#= f.input :interpolations %>
    
  <div class="form-group">
    <div class="col-sm-offset-3 col-sm-9">  
      <%= f.button :submit %>
    </div>
  </div>
<% end %>